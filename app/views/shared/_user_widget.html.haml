.user-widget-wrapper
  .user-widget
    .panel.wishes
      .icon
      - if current_user.present?
        = link_to '', wishlist_users_path
      - else
        = link_to '', new_user_session_path
    .panel.cart
      .icon
      .cart-items-counter 5
      = link_to '', '#'
    .panel.profile
      .icon
        - if current_user.present?
          = link_to '', profile_users_path
        - else
          = link_to '', new_user_session_path